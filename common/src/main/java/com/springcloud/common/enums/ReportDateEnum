import com.springcloud.common.enums;
public enum ReportDateEnum {
	//这里是可以自己定义的，方便与前端交互即可
    YEAR("year","年"),
    SEASON("season","季"),
    MONTH("month","月"),
    DAY("day","日");

    private String code;
    private String msg;

	XReportDateEnum(String code, String msg) {
        this.code = code;
        this.msg = msg;
    }

	public static XReportDateEnum getCode(String code) {
		XReportDateEnum[] imageFormatTypes = values();
		for (XReportDateEnum imageFormatType : imageFormatTypes) {
			if (imageFormatType.getCode().equals(code)) {
				return imageFormatType;
			}
		}
		return null;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	public static void main(String[] args) {
		XReportDateEnum year = XReportDateEnum.getCode("year");
		System.out.println(year == XReportDateEnum.YEAR);
	}

}
